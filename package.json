{
  "name": "fabricate-cli",
  "version": "0.1.0",
  "description": "A Library to create CLI's with ease",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm examples/index.ts "
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "fabricate",
    "fabricate-cli",
    "node-fabricate"
  ],
  "author": "Raman Sharma",
  "repository": {
    "type": "git",
    "url": "https://github.com/RamanSharma100/fabricate-cli.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.10",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0"
  },
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0"
  },
  "eslintConfig": {
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}