{
  "name": "fabricate-cli",
  "version": "0.2.0",
  "description": "A Library to create CLI's with ease",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm examples/index.ts "
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "cli",
    "fabricate",
    "fabricate-cli",
    "node-fabricate"
  ],
  "author": "Raman Sharma",
  "repository": {
    "type": "git",
    "url": "https://github.com/RamanSharma100/fabricate-cli.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0"
  },
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0"
  },
  "eslintConfig": {
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}